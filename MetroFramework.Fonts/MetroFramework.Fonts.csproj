<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyTitle>MetroFramework.Fonts.dll</AssemblyTitle>
    <Company>Sven Walter, Dennis Magno</Company>
    <Product>MetroFramework</Product>
    <Description>Metro UI Framework for .NET WinForms</Description>
    <Copyright>Copyright \x00a9 2011 Sven Walter, Dennis Magno.  All rights reserved.</Copyright>
    <AssemblyVersion>1.4.0.0</AssemblyVersion>
    <FileVersion>1.4.0.0</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup>
    <SignAssembly>true</SignAssembly>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>..\MetroFramework.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="Resources\Open Sans License.txt" />
    <EmbeddedResource Include="Resources\Open_Sans_Bold.ttf" />
    <EmbeddedResource Include="Resources\Open_Sans_Light.ttf" />
    <EmbeddedResource Include="Resources\Open_Sans.ttf" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\MetroFramework\MetroFramework.csproj" />
  </ItemGroup>
</Project>